@inproceedings{bocciolone:2013:application-of-new-optical-load,
	Abstract = {The contact force between pantograph and catenary is a parameter needed to evaluate the quality of the dynamic interaction of the two systems. The in-line measurements of this parameter have always been very hard due to the particular work condition of pantograph and catenary. High voltage (from 1.5 kV dc for underground trains to 25 kV ac for high-speed trains) is the main obstacle to be overcome for the measurement setup: if traditional sensors are used (electrical load cells), an adequate electrical insulation of the whole measurement setup is necessary, making it more complex and more expensive. In this work, the use of fiber Bragg gratings (FBG) load cells, designed on purpose, is presented. The main advantage of this kind of sensor is the intrinsic electrical insulation, which makes it very suitable for the measurement of pantograph-catenary contact force. In the following sections, laboratory tests and in-line tests using the optical load cells to measure the pantograph-catenary contact force will be discussed, proving the good work of the load cells and the possibility to obtain an easier measurement setup for the considered application.},
	Author = {Bocciolone, Marco and Bucca, Giuseppe and Collina, Andrea and Comolli, Lorenzo},
	Booktitle = {Proc. SPIE 8794, Fifth European Workshop on Optical Fibre Sensors},
	Date-Added = {2014-04-03 09:49:16 +0000},
	Date-Modified = {2014-04-03 17:28:29 +0000},
	Title = {Application of new optical load cells for in-line measurement of pantograph-catenary contact force},
	Year = {2013}}

